{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory1243"
		},
		"datalake1243_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake1243'"
		},
		"dataworkspace1243_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dataworkspace1243'"
		},
		"datalake1243_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake1243.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory1243_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake1243_accountKey": {
						"value": "[parameters('datalake1243_accountKey')]"
					},
					"dataworkspace1243_connectionString": {
						"value": "[parameters('dataworkspace1243_connectionString')]"
					},
					"datalake1243_properties_typeProperties_url": {
						"value": "[parameters('datalake1243_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}